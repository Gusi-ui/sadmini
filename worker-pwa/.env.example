# Configuración de Supabase
# Obtén estos valores de tu dashboard de Supabase (Settings > API)
VITE_SUPABASE_URL=https://tu-proyecto.supabase.co
VITE_SUPABASE_ANON_KEY=tu-clave-anonima
VITE_SUPABASE_SERVICE_ROLE_KEY=tu-clave-service-role

# Configuración de la aplicación
VITE_APP_NAME=Trabajadoras PWA
VITE_APP_VERSION=1.0.0

# Configuración del municipio
VITE_MUNICIPALITY=Mataró

# URL del panel de administración (opcional)
VITE_ADMIN_URL=https://tu-admin.vercel.app

# Configuración de notificaciones
VITE_ENABLE_NOTIFICATIONS=true

# Configuración de desarrollo
VITE_DEV_MODE=true
NODE_ENV=development

# Variables adicionales para scripts de configuración
SUPABASE_URL=https://tu-proyecto.supabase.co
SUPABASE_ANON_KEY=tu-clave-anonima
SUPABASE_SERVICE_ROLE_KEY=tu-clave-service-role