# Configuración de Supabase para desarrollo local
# IMPORTANTE: Para usar datos mock en desarrollo, usar valores de ejemplo

# URL de tu proyecto Supabase (valores de ejemplo para activar modo mock)
VITE_SUPABASE_URL=https://tu-proyecto.supabase.co

# Clave anónima de Supabase (valor de ejemplo para activar modo mock)
VITE_SUPABASE_ANON_KEY=tu-clave-anonima

# Configuración de la aplicación
VITE_APP_NAME=Aplicación para Trabajadoras
VITE_APP_VERSION=1.0.0
VITE_DEV_MODE=true
NODE_ENV=development

# URLs de otras aplicaciones
VITE_PWA_URL=http://localhost:5174
VITE_ADMIN_URL=http://localhost:5173

# Configuración específica para trabajadoras
VITE_MUNICIPALITY=Mataró
VITE_ENABLE_NOTIFICATIONS=true