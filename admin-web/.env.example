# Configuración de Supabase
# Obtén estos valores de tu dashboard de Supabase (Settings > API)
VITE_SUPABASE_URL=https://tu-proyecto.supabase.co
VITE_SUPABASE_ANON_KEY=tu-clave-anonima
VITE_SUPABASE_SERVICE_ROLE_KEY=tu-clave-service-role

# Configuración de la aplicación
VITE_APP_NAME=Panel de Administración - Cuidado Domiciliario
VITE_APP_VERSION=1.0.0

# Configuración opcional
# Nombre del municipio (por defecto: Mataró)
VITE_MUNICIPALITY=Mataró

# URL de la PWA de trabajadoras (opcional)
VITE_PWA_URL=https://tu-pwa.vercel.app

# Configuración de desarrollo
VITE_DEV_MODE=true
NODE_ENV=development

# Variables adicionales para scripts de configuración
SUPABASE_URL=https://tu-proyecto.supabase.co
SUPABASE_ANON_KEY=tu-clave-anonima
SUPABASE_SERVICE_ROLE_KEY=tu-clave-service-role
