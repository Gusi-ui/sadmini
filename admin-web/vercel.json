{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "VITE_APP_NAME": "Admin Panel - Cuidado Domiciliario",
    "VITE_APP_VERSION": "1.0.0",
    "VITE_PWA_URL": "https://trabajadoras.cuidado-domestico.com"
  }
}