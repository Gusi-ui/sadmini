# Variables de entorno para la aplicación administrativa
# Copia este archivo como .env.local y configura tus valores

# Configuración de Supabase (OBLIGATORIO)
VITE_SUPABASE_URL=your_supabase_project_url
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key

# Configuración de la aplicación
VITE_APP_NAME="Panel de Administración - Cuidado Doméstico"
VITE_APP_VERSION="1.0.0"

# Configuración de desarrollo
NODE_ENV=development
VITE_DEV_MODE=true

# Configuración de notificaciones (opcional)
VITE_ENABLE_NOTIFICATIONS=true
